@article{barsanti_3d_2022,
	title = {3D Reality-Based Survey and Retopology for Structural Analysis of Cultural Heritage},
	volume = {22},
	rights = {© 2022 by the authors.  Licensee {MDPI}, Basel, Switzerland. This article is an open access article distributed under the terms and conditions of the Creative Commons Attribution ({CC} {BY}) license (https://creativecommons.org/licenses/by/4.0/).  Notwithstanding the {ProQuest} Terms and Conditions, you may use this content in accordance with the terms of the License.},
	url = {https://www.proquest.com/docview/2756784194/abstract/9FEE61E753B8429BPQ/1},
	doi = {10.3390/s22249593},
	abstract = {Cultural heritage’s structural changes and damages can influence the mechanical behaviour of artefacts and buildings. The use of finite element methods ({FEM}) for mechanical analysis is largely used in modelling stress behaviour. The workflow involves the use of {CAD} 3D models and the use of non-uniform rational B-spline ({NURBS}) surfaces. For cultural heritage objects, altered by the time elapsed since their creation, the representation created with the {CAD} model may introduce an extreme level of approximation, leading to wrong simulation results. The focus of this work is to present an alternative method intending to generate the most accurate 3D representation of a real artefact from highly accurate 3D reality-based models, simplifying the original models to make them suitable for finite element analysis ({FEA}) software. The approach proposed, and tested on three different case studies, was based on the intelligent use of retopology procedures to create a simplified model to be converted to a mathematical one made by {NURBS} surfaces, which is also suitable for being processed by volumetric meshes typically embedded in standard {FEM} packages. This allowed us to obtain {FEA} results that were closer to the actual mechanical behaviour of the analysed heritage asset.},
	pages = {9593},
	number = {24},
	journaltitle = {Sensors},
	author = {Barsanti, Sara Gonizzi and Guagliano, Mario and Rossi, Adriana},
	urldate = {2024-10-29},
	date = {2022},
	note = {Number: 24
Num Pages: 9593
Place: Basel, Switzerland
Publisher: {MDPI} {AG}},
	keywords = {Finite element method, Approximation, Finite element analysis, 3D modelling, 3D survey, Accuracy, Architecture, B spline functions, Construction, convergence analysis, Cultural heritage, Cultural resources, Documentation, {FEA}, Heritage, Historic artifacts, Mechanical properties, {NURBS}, Representations, retopology, Software, Structural analysis, Workflow},
	file = {Full Text PDF:C\:\\Users\\wvh20\\Zotero\\storage\\ZMCZF3C3\\Barsanti et al. - 2022 - 3D Reality-Based Survey and Retopology for Structu.pdf:application/pdf},
}

