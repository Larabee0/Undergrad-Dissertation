@inproceedings{decoro_real-time_2007,
	location = {Seattle Washington},
	title = {Real-time mesh simplification using the {GPU}},
	isbn = {978-1-59593-628-8},
	url = {https://dl.acm.org/doi/10.1145/1230100.1230128},
	doi = {10.1145/1230100.1230128},
	abstract = {Recent advances in real-time rendering have allowed the {GPU} implementation of traditionally {CPU}-restricted algorithms, often with performance increases of an order of magnitude or greater. Such gains are achieved by leveraging the large-scale parallelism of the {GPU} towards applications that are well-suited for these streaming architectures. By contrast, mesh simpliﬁcation has traditionally been viewed as a non-interactive process not readily amenable to {GPU} acceleration. We demonstrate how it becomes practical for real-time use through our method, and that the use of the {GPU} even for ofﬂine simpliﬁcation leads to signiﬁcant increases in performance. Our approach for mesh decimation adopts a vertexclustering method to the {GPU} by taking advantage of a new addition to the rendering pipeline - the geometry shader stage. We present a novel general-purpose data structure designed for streaming architectures called the probabilistic octree, which allows for much of the ﬂexibility of ofﬂine implementations, including sparse encoding and variable level-of-detail. We demonstrate successful use of this data structure in our {GPU} implementation of mesh simpliﬁcation. We can generate adaptive levels of detail by applying non-linear warping functions to the cluster map in order to improve resulting simpliﬁcation quality. Our {GPU}-accelerated approach enables simultaneous construction of multiple levels of detail and outof-core simpliﬁcation of extremely large polygonal meshes.},
	eventtitle = {I3D07: Symposium on Interactive 3D Graphics and Games 2007},
	pages = {161--166},
	booktitle = {Proceedings of the 2007 symposium on Interactive 3D graphics and games},
	publisher = {{ACM}},
	author = {{DeCoro}, Christopher and Tatarchuk, Natalya},
	urldate = {2024-11-20},
	date = {2007-04-30},
	langid = {english},
	keywords = {Simplification Algorithm},
	file = {DeCoro and Tatarchuk - 2007 - Real-time mesh simplification using the GPU.pdf:C\:\\Users\\wvh20\\Zotero\\storage\\P5MF84KW\\DeCoro and Tatarchuk - 2007 - Real-time mesh simplification using the GPU.pdf:application/pdf},
}

