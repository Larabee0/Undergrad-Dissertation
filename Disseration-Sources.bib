@inproceedings{peyrot_aliasing-free_2014,
	title = {Aliasing-free simplification of surface meshes},
	url = {https://ieeexplore.ieee.org/document/7025948/?arnumber=7025948},
	doi = {10.1109/ICIP.2014.7025948},
	abstract = {We propose in this paper a robust simplification technique, which preserves geometric features such as sharp edges or corners from original surfaces. To achieve this goal, our simplification process relies on a detection tool that enables to preserve the sharp features during the three subsequent steps: a Poisson disk sampling that intelligently reduces the number of vertices of the initial mesh; the meshing of the samples that aligns the edges along the feature lines; and a constrained relaxation step that improves the shape of the triangles of our final simplified mesh. Experimental results show that our method always produces valid meshes without aliasing artifacts, and without giving up the shape fidelity and quality of the mesh elements.},
