@thesis{stiaan_uyttersprot_mesh_2018,
	title = {Mesh Compression and Procedural Content Generation: Finding Common Ground for an Improved 3D Representation},
	url = {https://libstore.ugent.be/fulltxt/RUG01/002/495/091/RUG01-002495091_2018_0001_AC.pdf},
	abstract = {Abstract—3D models are used in many different applications.
Because their use is widespread, many distinct techniques
exist for creating, altering and optimising these models. Two
of the most researched techniques are mesh compression and
procedural content generation ({PCG}). Mesh compression is used
to compress existing models into smaller sized formats used for
transmission and storage. {PCG} is used to create content like
models algorithmically.
Even though these techniques are researched plentiful and
often used individually, no methods have been proposed for
combining both of them. The purpose of this study is to find a
possible manner of combining these techniques to gain a better
or more efficient 3D representation than when one of them is
used individually. A novel method is proposed that splits up the
3D model into two parts: a structured part and an unstructured
part. The structured part can then be generated using {PCG}
while the unstructured part is compressed and decompressed
using classic progressive mesh compression. These two parts
then are combined to form the final 3D model.
This novel method provides an alternative to classic progressive
mesh compression algorithms. Compared to these algorithms,
the novel method requires less memory space for storage
and provides better visual representations, but requires more
computation time for generating the complete model. Results
suggest that the novel method is beneficial when models have
a smaller sized unstructured part and bigger structured parts,
while progressive mesh compression is the better solution for
models with a larger unstructured part.},
	pagetotal = {77},
	institution = {Ghent University},
	type = {Masters Disseration},
	author = {{Stiaan Uyttersprot}},
	urldate = {2024-11-26},
	date = {2018},
	keywords = {Simplification Algorithm},
	file = {RUG01-002495091_2018_0001_AC.pdf:C\:\\Users\\wvh20\\Zotero\\storage\\65DF56C4\\RUG01-002495091_2018_0001_AC.pdf:application/pdf},
}

